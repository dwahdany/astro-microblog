# PostHog Analytics
# Get your API key from https://posthog.com
PUBLIC_POSTHOG_KEY=phc_your_posthog_api_key_here

# Giscus Comments
# Get these values from https://giscus.app after setting up your repo
PUBLIC_GISCUS_REPO=your-username/your-repo
PUBLIC_GISCUS_REPO_ID=R_your_repo_id
PUBLIC_GISCUS_CATEGORY=Announcements
PUBLIC_GISCUS_CATEGORY_ID=DIC_your_category_id

# Optional: Force analytics in development
PUBLIC_FORCE_ANALYTICS=false
